    "prosemirror-view": "^1.27.0",
    "rollup": "^2.74.0",
    "rollup-plugin-typescript2": "^0.31.2",
    "ts-jest": "27.1.4",
    "tslib": "^2.4.0",
    "typescript": "^4.6.4"
  },
  "peerDependencies": {
    "prosemirror-model": ">=1.14.0",
    "prosemirror-state": ">=1.3.0",
    "prosemirror-transform": ">=1.3.0",
    "prosemirror-view": ">=1.18.0"
  },
  "dependencies": {
    "debug": "^4.3.4"
  }
