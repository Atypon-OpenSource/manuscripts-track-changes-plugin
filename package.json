  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "type": "module",
  "types": "dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    "./src/styles.css": "./src/styles.css",
    ".": "./dist/index.js"
  },
  "files": [
    "dist",
    "src/styles.css"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "rollup -c",
    "watch": "rollup -cw",
    "test": "jest --runInBand",
    "format": "prettier --write \"*.+(js|json|yml|yaml|ts|md|graphql|mdx)\" src/ test/",
    "typecheck": "tsc --project tsconfig.test.json --noEmit",
    "lint": "eslint --cache --ext .js,.ts, ./src ./test",
    "lint:fix": "eslint --fix --ext .js,.ts, ./src ./test"
  },
  "devDependencies": {
    "@manuscripts/manuscript-transform": "^0.49.4",
    "@rollup/plugin-commonjs": "^22.0.0",
    "@types/debug": "^4.1.7",
    "@types/jest": "27.5.1",
    "@types/node": "^17.0.35",
    "jest": "27.5.1",
    "jest-environment-jsdom": "27.5.1",
    "jsdom": "^19.0.0",
    "prosemirror-commands": "^1.3.0",
    "prosemirror-example-setup": "^1.2.1",
    "prosemirror-history": "^1.3.0",
    "prosemirror-keymap": "^1.2.0",
    "prosemirror-model": "^1.18.1",
    "prosemirror-schema-list": "^1.2.0",
