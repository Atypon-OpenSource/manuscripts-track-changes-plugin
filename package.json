{
  "name": "@manuscripts/track-changes-plugin",
  "version": "1.2.0",
  "author": "Atypon Systems LLC",
  "license": "Apache-2.0",
  "homepage": "https://github.com/Atypon-OpenSource/manuscripts-quarterback/tree/main/quarterback-packages/track-changes-plugin",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "type": "module",
  "types": "dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    "./src/styles.css": "./src/styles.css",
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist",
    "src/styles.css"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "rollup -c",
    "watch": "rollup -cw",
    "test": "jest --runInBand",
    "format": "prettier --write \"*.+(js|json|yml|yaml|ts|md|graphql|mdx)\" src/ test/",
    "typecheck": "tsc --project tsconfig.test.json --noEmit",
    "lint": "eslint --cache --ext .js,.ts, ./src ./test",
    "lint:fix": "eslint --fix --ext .js,.ts, ./src ./test"
  },
  "devDependencies": {
    "@manuscripts/data": "^0.1.12",
    "@manuscripts/json-schema": "^1.1.0",
    "@manuscripts/transform": "^1.0.1",
    "@rollup/plugin-commonjs": "^22.0.2",
    "@types/debug": "^4.1.7",
    "@types/jest": "27.5.1",
    "@types/node": "^18.7.18",
    "jest": "27.5.1",
    "jest-environment-jsdom": "27.5.1",
    "jsdom": "^20.0.0",
    "rollup": "^2.79.0",
    "rollup-plugin-typescript2": "^0.34.0",
    "ts-jest": "27.1.4",
    "tslib": "^2.4.0",
    "typescript": "^4.8.3"
  },
  "peerDependencies": {
    "prosemirror-model": ">=1.14.0",
    "prosemirror-state": ">=1.3.0",
    "prosemirror-transform": ">=1.3.0",
    "prosemirror-view": ">=1.18.0"
  },
  "dependencies": {
    "prosemirror-commands": "^1.5.0",
    "prosemirror-example-setup": "^1.2.1",
    "prosemirror-history": "^1.3.0",
    "prosemirror-keymap": "^1.2.0",
    "prosemirror-model": "^1.18.3",
    "prosemirror-schema-list": "^1.2.2",
    "prosemirror-state": "^1.4.2",
    "prosemirror-transform": "^1.7.0",
    "prosemirror-view": "^1.29.1",
    "debug": "^4.3.4"
  }
}
